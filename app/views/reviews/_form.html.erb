<%= simple_form_for([@play, @play.reviews.build]) do |f| %>
    <div id="rating-form">
        <label>Rating</label>
    </div>
        <%= f.input :comment %>
        <%= f.button :submit, :class => "btn btn-custom" %>
<% end %>

<script>
    $('#rating-form').raty({
        path: '/assets/',
        scoreName: 'review[rating]'
    });
</script>